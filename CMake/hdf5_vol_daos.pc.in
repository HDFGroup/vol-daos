# This gives access to the hdf5 daos header files
prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@CMAKE_INSTALL_PREFIX@
libdir=@HDF5_VOL_DAOS_INSTALL_LIB_DIR@
includedir=@HDF5_VOL_DAOS_INSTALL_INCLUDE_DIR@

Name: HDF5 DAOS
Description: HDF5 DAOS VOL connector
Version: @HDF5_VOL_DAOS_PACKAGE_VERSION@
URL: http://www.hdfgroup.org
Requires: @HDF5_VOL_DAOS_PKG_DEPENDENCIES@
Libs: -L${libdir} @HDF5_VOL_DAOS_LIBRARIES@
Libs.private: @HDF5_VOL_DAOS_LIB_DEPENDENCIES@
Cflags: -I${includedir} @HDF5_VOL_DAOS_INCLUDE_DEPENDENCIES@
